    "react-select": "^5.7.4",
    "vite-plugin-svgr": "^3.2.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "1.5.0",
    "@tailwindcss/forms": "0.5.3",
