    "@types/react-select": "^5.0.1",
    "design-system-react": "cfpb/design-system-react",
    "immer": "^10.0.2",
    "keycloak-js": "^21.1.1",
    "less": "^4.1.3",
    "oidc-client-ts": "^2.2.4",
